{
  "version": 2,
  "name": "project-id-on-vercel-dashboard",
  "public": true,
  "builds": [
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "env": {
      "DATABASE_URL": "${env.DATABASE_URL}",
      "REPLICATE_API_TOKEN": "${env.REPLICATE_API_TOKEN}",
      "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY": "${env.NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY}",
      "CLERK_SECRET_KEY": "${env.CLERK_SECRET_KEY}",
      "NEXT_PUBLIC_CLERK_SIGN_IN_URL": "${env.NEXT_PUBLIC_CLERK_SIGN_IN_URL}",
      "NEXT_PUBLIC_CLERK_SIGN_UP_URL": "${env.NEXT_PUBLIC_CLERK_SIGN_UP_URL}",
      "NEXT_PUBLIC_PAYPAL_CLIENT_ID": "${env.NEXT_PUBLIC_PAYPAL_CLIENT_ID}"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/old-page",
      "destination": "/new-page",
      "permanent": true
    }
  ]
}
