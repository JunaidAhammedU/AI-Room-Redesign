{
    "version": 2,
    "name": "project-id-on-vercel-dashboard",
    "public": true,
    "builds": [
      { "src": "next.config.js", "use": "@vercel/next" }
    ],
    "functions": {
        "api/**/*.js": {
          "maxDuration": 10
        }
      },
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/api/:path*"
      }
    ],
    "redirects": [
      {
        "source": "/old-page",
        "destination": "/new-page",
        "permanent": true
      }
    ]
  }
